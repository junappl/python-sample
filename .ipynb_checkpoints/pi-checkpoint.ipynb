   "source": [
    "import random\n",
    "\n",
    "# 乱数の発生回数\n",
    "imax = 10000000\n",
    "\n",
    "n = 0\n",
    "\n",
    "for i in range(imax):\n",
    "    x = random.random()\n",
    "    y = random.random()\n",
    "\n",
    "    if x * x + y * y < 1.0:                   \n",
    "        n += 1.0\n",
    "\n",
    "pi = n / imax * 4.0\n",
    "print(pi)"
   ]
